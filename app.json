{
  "expo": {
    "name": "withPet-expo",
    "slug": "withPet-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "com.withpet"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.reactjs.native.example.withPet"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
      
    }
  }
}
